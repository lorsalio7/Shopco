      .brands__ticker.splide__list
        .splide__slide
          img(src="img/versace-logo.svg", alt="Versace logo").brands__logo
        .splide__slide
          img(src="img/zara-logo.svg", alt="Zara logo").brands__logo
        .splide__slide
          img(src="img/gucci-logo.svg", alt="Gucci logo").brands__logo
        .splide__slide
          img(src="img/prada-logo.svg", alt="Prada logo").brands__logo
        .splide__slide
          img(src="img/calvin-klein-logo.svg", alt="Calvin Klein logo").brands__logo
